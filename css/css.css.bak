 h1 {
    font-family: "Minecraft"
   }

html {
	position: fixed;
	top: 0%;
	bottom: 0%;
	right: 0%;
	left: 0%;
	box-shadow: inset 0 0 99999px #FF8C00;
	background-image: linear-gradient(to right, #FFFFFF, #0033CC);
	background: url(fon.png) center center;
	background-repeat: no-repeat;
	background-size: 100% 100%;

}

body {
	position: fixed;
	top: 0%;
	bottom: 0%;
	right: -7.8px;
	left: -7.8px;
}

.C30 {
	    margin: 0;
        padding: 0;
        list-style-type: none;
        width: 100%;
		height: 65px;
      }
.C30 li { 
	display: inline;
}
.C30 a {
    color: #fff;
	position: relative;
	font-family: Minecraft, 'Minecraft', Minecraft Rus;
	top: 50%;
	bottom: 40%;
	width: 225px;
    text-decoration: none;
	font-size: 140%;
    font-weight: bold;
    display: inline-block;
	text-align: center;
}
.C30 a:hover {
    color: #FF8C00;
	  }

.F20 {
	width: 65%;
	float: right;
}

.A5 {
	position: absolute;
	top: -1%;
	width: 100%;
    height: 90px;
	background: rgba(0, 0, 0, 0.35);
}

.E15 {
	
	width: 34.5%;
	float: left;
	background: url(images/GC4USQ.png) 100px 5px;
	background-size: 488px 100%;
	background-repeat: no-repeat;
	height: 80px;
	top: 10px;
}

a.D7 {
  position: absolute;
  	width: 230px;
  font-weight: bold;
  font-family: Minecraft, 'Minecraft', Minecraft Rus;
  color: white;
  text-decoration: none;
  text-shadow: 0 2px 1px rgba(0, 0, 0, 0.3);
  text-align: center;
  user-select: none;
  padding: 1.4em 2em;
  outline: none;
  background: #FF8C00;
  background-size: 100% 100%, auto;
  background-position: 50% 50%;
  box-shadow: 0 -4px rgb(170 70 0 / 65%) inset, 0 4px rgb(255 180 0 / 99%) inset, -4px 0 rgb(255 165 0 / 55%) inset, 4px 0 rgb(170 70 0 / 75%) inset;
  transition: 0.2s;
}
a.D7:hover {
  background-size: 140% 100%, auto;
  box-shadow: 0 -4px rgb(20 20 20 / 65%) inset, 0 4px rgb(115 115 115 / 99%) inset, -4px 0 rgb(145 145 145 / 50%) inset, 4px 0 rgb(15 15 15 / 75%) inset;
  background: rgba(50, 50, 50, 1);
  transition: 0.4s;
}
a.D7:active {
  top: 1px;
  color: #ffdead; 
}

.R12 {
	position: relative;
	width: 100%;
	height: 100%;
	top: 30%;
	right: 0%;
	bottom: 0%;
	left: 20%;
}

.R322 {
	position: absolute;
	background-color: rgba(0,0,0,0.2);
	top: 25%;
	left: 10%;
	right: 10%;
	bottom: auto;
	-moz-border-radius: 20px;
	-webkit-border-radius: 20px;
	border-radius: 20px;
}
.R322 table, th, td {
	position: relative;
	top: 0%;
	left: 0%;
	right: 0%;
	bottom: auto;
    border-collapse: collapse;
}

.R322 th {
	padding: 5px;
    text-align: center;
	background: rgba(0, 0, 0, 0);
	font-family: Pompadur, 'Comic Sans MS', cursive;
	color: #FF8C07;
	font-size: 260%;
	height: 80px;
	border: none;
}

.R322 td {
	padding: 7px;
	width: 520px;
	height: auto;
}

.R323 {
	padding: 5px;
    text-align: left;
	font-family: Pompadur, 'Comic Sans MS', cursive;
	color: rgba(255, 255, 255, 1);
	font-size: 150%;
}

input[type="D7"]{
  font-family: Pompadur, 'Comic Sans MS', cursive;
}
